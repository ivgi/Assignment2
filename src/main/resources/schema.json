{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Input validation",
  "description": "Game of Life Input",
  "type": "object",
  "id": "schema",
  "properties": {
    "numIterations": {
      "description": "Number of iterations of the game board.",
      "type": "integer",
      "maximum": 100
    },
    "liveCellsPercent": {
      "type": "integer",
      "maximum": 100
    }
  },
  "required": [
    "numIterations",
    "liveCellsPercent"
  ]
}
